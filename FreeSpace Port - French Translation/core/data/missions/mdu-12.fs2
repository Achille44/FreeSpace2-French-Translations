$Num stages: 7

$Formula: ( is-goal-true-delay "Send uplink" 0 ) 
$Multi text
    XSTR("Congratulations everyone for an outstanding job.  Knocking out the Alexander Bell's radar allowed the Bell to transmit our information to command.  If a few more minutes had passed, the secrets would have died with us.  As you probably noticed, our names have been cleared.  Excellent work.", 7043)
$end_multi_text
$Voice: none.wav
$Recommendation text:
    XSTR("", 606)
$end_multi_text

$Formula: ( has-departed-delay 0 "GTD Nepenthe" ) 
