#Briefing
$start_briefing
$num_stages: 5
$start_stage
$multi_text
 XSTR("At 1430 hours, the Warlock entered the Gienah Cygni system.  Upon entering the system, our long range sensors were jammed.  We cannot adequately assess the situation in this system until the jamming ceases.", 5939)
$end_multi_text
$voice: e3m4_b1.ogg
$camera_pos: 962.440247, 1443.884399, 5217.696289
$camera_orient:
	-0.854122, -0.000001, 0.520073,
	-0.199832, 0.923235, -0.328185,
	-0.480149, -0.384237, -0.788555
$camera_time: 500
$num_lines: 0
$num_icons: 0
$Flags: 0
$Formula: ( true ) 
$end_stage
$start_stage
$multi_text
 XSTR("We have managed to track the source of the jamming to a region near a giant gas planet caught in the star's gravitational field.  We suspect that a Shivan communications node is responsible for the jamming of our systems, but we cannot be sure.", 5940)
$end_multi_text
$voice: e3m4_b2.ogg
$camera_pos: 962.440247, 1443.884399, 5217.696289
$camera_orient:
